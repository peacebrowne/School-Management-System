    <!-- STUDENT SCRIPT -->
    <script>
      const getChartOptions = (maleStudents, femaleStudents) => {
        const totalMale = maleStudents.reduce((a, b) => a + b, 0);
        const totalFemale = femaleStudents.reduce((a, b) => a + b, 0);
        const totalStudents = totalMale + totalFemale;

        return {
          series: [totalMale, totalFemale],
          colors: ["#00DFA2", "#FF0060"],
          chart: {
            height: 380,
            width: "100%",
            type: "donut",
          },
          labels: ["Male Students", "Female Students"],
          plotOptions: {
            pie: {
              donut: {
                labels: {
                  show: true,
                  name: {
                    show: true,
                    fontFamily: "Inter, sans-serif",
                    offsetY: 20,
                  },
                  total: {
                    showAlways: true,
                    show: true,
                    label: "Total Students",
                    fontFamily: "Inter, sans-serif",
                    formatter: function (w) {
                      return totalStudents;
                    },
                  },
                  value: {
                    show: true,
                    offsetY: -20,
                    formatter: function (value) {
                      return value;
                    },
                  },
                },
                size: "55%",
              },
            },
          },
          grid: {
            padding: {
              top: -2,
            },
          },
          legend: {
            position: "bottom",
            fontFamily: "Inter, sans-serif",
          },
          annotations: {
            position: "bottom",
            yaxis: [
              {
                y: -50,
                borderColor: "#999",
                label: {
                  text: "Total Male Students: " + totalMale,
                  style: {
                    color: "#1C64F2",
                    background: "none",
                    fontSize: "14px",
                    fontFamily: "Inter, sans-serif",
                  },
                },
              },
              {
                y: -80,
                borderColor: "#999",
                label: {
                  text: "Total Female Students: " + totalFemale,
                  style: {
                    color: "#E74694",
                    background: "none",
                    fontSize: "14px",
                    fontFamily: "Inter, sans-serif",
                  },
                },
              },
            ],
          },
        };
      };

      // Example data for male and female students
      const maleStudentsData = [105000]; // Total male students
      const femaleStudentsData = [45000]; // Total female students

      if (
        document.getElementById("donut-chart") &&
        typeof ApexCharts !== "undefined"
      ) {
        const chart = new ApexCharts(
          document.getElementById("donut-chart"),
          getChartOptions(maleStudentsData, femaleStudentsData)
        );
        chart.render();
      }
    </script>
